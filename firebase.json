{
  "hosting": {
    "public": "dist",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/terms*",
        "function": "/terms.html"
      },
      {
        "source": "/privacy",
        "function": "/privacy.html"
      }
    ]
  }
}
